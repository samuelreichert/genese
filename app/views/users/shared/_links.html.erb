<% first = true %>
<%- if controller_name != 'sessions' %>
  <%- if !first %>
    |
  <% end -%>
  <%= link_to "Login", new_session_path(resource_name) %>
  <% first = false %>
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%- if !first %>
    |
  <% end -%>
  <%= link_to "Cadastre-se", new_registration_path(resource_name) %>
  <% first = false %>
<% end -%>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%- if !first %>
    |
  <% end -%>
  <%= link_to "Esqueceu sua senha?", new_password_path(resource_name) %>
  <% first = false %>
<% end -%>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%- if !first %>
    |
  <% end -%>
  <%= link_to "Não recebeu suas instruções de confirmação?", new_confirmation_path(resource_name) %>
  <% first = false %>
<% end -%>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%- if !first %>
    |
  <% end -%>
  <%= link_to "Não recebeu as instruções de desbloqueio?", new_unlock_path(resource_name) %>
  <% first = false %>
<% end -%>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%- if !first %>
      |
    <% end -%>
    <%= link_to "Login com #{provider.to_s.titleize}", omniauth_authorize_path(resource_name, provider) %>
    <% first = false %>
  <% end -%>
<% end -%>
